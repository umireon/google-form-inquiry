{
  "name": "auto-inquiry",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:html": "pug -P ./src/index.pug -o ./build",
    "build:bundle": "cross-env NODE_ENV=production webpack bundle=./src/client",
    "build:App": "cross-env NODE_ENV=production webpack App=./src/server/App.js",
    "build:Code": "babel ./src/server/Code.js -o ./build/Code.js",
    "dist": "run-p dist:**",
    "dist:debug:server": "gulp dist:debug:server",
    "dist:debug:client": "cp build/App.js build/Code.js dist/debug/src",
    "dist:release:server": "gulp dist:release:server",
    "dist:release:client": "cp build/App.js build/Code.js dist/release/src",
    "upload:debug": "cd dist/debug && gapps upload",
    "upload:release": "cd dist/release && gapps upload",
    "deploy:debug": "run-s build dist upload:debug",
    "deploy:release": "run-s build dist upload:release",
    "webpack:serve": "webpack-dev-server --config webpack-dev-server.config.js --hot --inline",
    "eslint": "eslint src/client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "bootstrap": "^3.3.7",
    "cross-env": "^2.0.0",
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^6.1.2",
    "font-awesome": "^4.6.3",
    "gulp": "^3.9.1",
    "gulp-html-replace": "^1.6.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-inline": "^0.1.1",
    "history": "^3.0.0",
    "lodash": "^4.15.0",
    "mustache": "^2.2.1",
    "node-google-apps-script": "^1.1.5",
    "npm-run-all": "^3.0.0",
    "pug": "^2.0.0-beta5",
    "pug-cli": "^1.0.0-alpha6",
    "react": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-bootstrap-switch": "^3.4.5",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.11.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  }
}
